{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        },
        "exclude": ["@babel/plugin-transform-block-scoping"],
        "useBuiltIns": "entry",
        "corejs": "3.8"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-spread",
    "@babel/plugin-transform-parameters",
    "transform-commonjs"
  ]
}
